{
  "version": 2,
  "builds": [
    {
      "src": "YaadLife-Lite/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "YaadVibe/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/YaadLife-Lite/index.html"
    },
    {
      "src": "/YaadVibe",
      "dest": "/YaadVibe/build/index.html"
    },
    {
      "src": "/YaadVibe/(.*)",
      "dest": "/YaadVibe/build/$1"
    }
  ]
}
